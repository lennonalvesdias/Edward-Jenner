{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "CorsSettings": {
    "PolicyName": "_corsPolicy",
    "Urls": "http://localhost:8080"
  },
  "MongoConnection": {
    "ConnectionString": "mongodb://{0}:{1}@mongo:27017/edwardjenner?authSource=admin",
    "User": "MongoConnection.User",
    "Password": "MongoConnection.Password",
    "Database": "edwardjenner",
    "IsSSL": false
  },
  "TokenConfigurations": {
    "Audience": "Clients-API-EdwardJenner",
    "Issuer": "API-EdwardJenner",
    "Seconds": 60,
    "FinalExpiration": 120
  },
  "RedisConnection": {
    "Host": "redis",
    "Port": 6379,
    "Seconds": 60000
  },
  "GoogleSettings": {
    "ApiKey": "GoogleSettings.ApiKey"
  }
}